<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.xkx.mapper.MovieContentMapper">
    <select id="selectMovieContentByMovieId" parameterType="Integer" resultType="movieContent">
        select * from movieContent where movieId=#{movieId}
    </select>
</mapper>